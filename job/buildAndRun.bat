@echo off
ECHO Running Maven clean+install...
call mvn clean install > mvn-output 2>&1
FINDSTR /L /S /I /N /C:"Failed to execute goal" mvn-output
ECHO Running Docker build...
call docker build -t myjob . > docker-output 2>&1
ECHO Running Docker container: myjob...
call docker run --rm -ti --name job myjob